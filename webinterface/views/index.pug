extends layout/layout

block content
  div(class="container")
    div(class="page-header")
      h1= title
    h3 Programs
    div(class="row")
      each f in functions
        div(class="col-md-4")
          h3= f.name
          p= f.description
          a(href= '/scripts/' + f.api)
            button(type='button', class="btn-default")= f.executionName
